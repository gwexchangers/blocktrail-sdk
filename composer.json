{
    "name": "gwexchangers/blocktrail-sdk-symfony4",
    "description": "PHP SDK for Blocktrail API - PHP 7.1+ and Symfony 4+",
    "authors": [
        {
            "name": "Wanna Launch S.A.",
            "email": "wannalauch.dev@gmail.com"
        },
    ],
    "autoload": {
        "psr-4": {
            "Blocktrail\\SDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blocktrail\\SDK\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-intl": "*",
        "ext-gmp": "*",
        "ext-bcmath": "*",
        "ext-gd": "*",
        "ext-dom": "*",
        "ext-curl": "*",
        "bitwasp/bitcoin": "^1.0",
        "btccom/bitwasp-bitcoin-bch-addon": "^0.0.2",
        "btccom/justencrypt": "^0.2.0",
        "btccom/cashaddress": "^0.0.3",
        "mdanter/ecc": "0.5.*",
        "guzzlehttp/guzzle": "6.*",
        "99designs/http-signatures-guzzlehttp": "2.*",
        "rych/hash_pbkdf2-compat": "^2.0",
        "ramsey/array_column": "~1.1",
        "dompdf/dompdf" : "0.8.*",
        "blocktrail/cryptojs-aes-php": "0.1.*",
        "spomky-labs/php-aes-gcm": "^2.0",
        "symfony/http-foundation": "4.*",
        "endroid/qr-code": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.8",
        "mockery/mockery": "^1.2"
    }
}
